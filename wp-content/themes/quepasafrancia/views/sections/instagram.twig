<div class="container">
    <div class="row">
        <div class="col-12">
            <a href="{{instagram.account.url}}">{{instagram.account.title}}</a>
        </div>
    </div>
    <div class="row">
        {% for image in instagram.images %}
            <div class="col-lg-4">
                <a href="{{image.link}}">
                    <img src="{{Image(image.image).src}}" alt="">
                </a>
            </div>
        {% endfor %}
    </div>
</div>